############## ALIAS REMOTE FILE SHARING ################

# IP & Network functions
get_device_ip() {
  ip route get 8.8.8.8 2>/dev/null | awk '{print $7; exit}' | grep -v '127\.'
}

infostart() {
  echo "Current directory: $(pwd)"
  echo "------------------------------"
  echo "| Network status:"
  for dev in eth0 tun0; do
    ip=$(ip -4 addr show $dev 2>/dev/null | awk '/inet / && $2 !~ /127\./ {split($2,a,"/"); print a[1]}' | head -1 | sed 's/\x1B\[[0-9;]*[JKmsu]//g')
    if [ -n "$ip" ]; then
      echo "------------------------------"
      printf "| \033[0;35m  %s: up (%s)\033[0m\n" "$dev" "$ip"
    else
      echo "------------------------------"
      printf "| \033[0;35m  %s: down\033[0m\n" "$dev"
      echo "------------------------------"
      [ "$dev" = "tun0" ] && printf "| \033[0;35m  No OpenVPN\033[0m\n"
      echo "------------------------------"
    fi
  done
}

